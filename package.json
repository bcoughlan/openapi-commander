{
  "name": "openapi-commander",
  "version": "3.1.0",
  "description": "Generate a commander.js CLI tool from an OpenAPI definition.",
  "exports": "src/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "openapi",
    "openapi3",
    "commander",
    "cli",
    "api",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bcoughlan/openapi-commander"
  },
  "files": [
    "bin",
    "dist",
    "resources"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openapi-commander": "bin/run.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src resources/*.js"
  },
  "author": "Barry Coughlan",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "commander": "^11.0.0",
    "lodash": "^4.17.21",
    "openapi-sampler": "^1.3.1",
    "swagger2openapi": "^7.0.8"
  },
  "overrides": {
    "supports-color": "8.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-jest": "^29.5.0",
    "eslint": "^8.36.0",
    "eslint-plugin-jest": "^27.2.1",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "tmp-promise": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
